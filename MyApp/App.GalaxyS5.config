<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
    <fourth driverType="Chrome" serverUrl="" xdt:Transform="Replace">
        <timeout driverCommands="00:00:59" defaultWait="00:00:59" />
        <reporting keepScreenshot="false" takeScreenshotOnError="true" />
        <mobile deviceName="Galaxy S5" />
        <capabilities>
            <add key="--start-maximized" />
        </capabilities>
    </fourth>
    <appSettings xdt:Transform="Replace">
        <add key="Website.Url" value="https://fourth-qa.herokuapp.com/" />
    </appSettings>
</configuration>