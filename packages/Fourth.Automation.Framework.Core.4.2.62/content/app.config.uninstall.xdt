<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <configSections>
    <section name="fourth" type="Fourth.Automation.Framework.Configuration.FourthSection, Fourth.Automation.Framework.Core" xdt:Locator="Condition([@name='fourth'])" xdt:Transform="Remove"/>
  </configSections>

  <configSections xdt:Locator="Condition(count(*) = 0)" xdt:Transform="Remove" />

  <fourth driverType="" serverUrl="" driverDirectory="" xdt:Locator="Condition([@driverType!='' or @serverUrl!='' or @driverDirectory!=''])" xdt:Transform="Remove">
  </fourth>

  <fourth driverType="" serverUrl="" driverDirectory="" xdt:Transform="Remove">
  </fourth>

  <specFlow>
    <stepAssemblies>
      <stepAssembly assembly="Fourth.Automation.Framework.Core" xdt:Locator="Condition([@stepAssembly='Fourth.Automation.Framework.Core'])" xdt:Transform="Remove"/>
      <stepAssembly assembly="Fourth.Automation.SpecFlow.Extensions" xdt:Locator="Condition([@stepAssembly='Fourth.Automation.SpecFlow.Extensions'])" xdt:Transform="Remove"/>
    </stepAssemblies>
    <plugins>
      <add name="SpecFlow.Autofac" type="Runtime" xdt:Transform="Remove"/>
    </plugins>
  </specFlow>

</configuration>